{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": ".",
    "outDir": "dist",
    "noImplicitThis": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "resolveJsonModule": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "strict": true
  }
}
